<?xml version="1.0" encoding="utf-8"?>
<MixxxMIDIPreset schemaVersion="1" mixxxVersion="1.7.0+">
	<info>
		<name>American Audio VMS4</name>
		<author>xorik</author>
		<description>Alternative mapping for VMS4</description>
	</info>
	<controller id="American Audio VMS4">
		<controls>
			<!-- cue -->
			<control>
				<group>[Channel1]</group>
				<key>cue_default</key>
				<status>0x90</status>
				<midino>0x0c</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>cue_default</key>
				<status>0x80</status>
				<midino>0x0c</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>cue_default</key>
				<status>0x90</status>
				<midino>0x2e</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>cue_default</key>
				<status>0x80</status>
				<midino>0x2e</midino>
			</control>
			<!-- play/pause -->
			<control>
				<group>[Channel1]</group>
				<key>play</key>
				<status>0x90</status>
				<midino>0x0d</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>play</key>
				<status>0x90</status>
				<midino>0x2f</midino>
			</control>
		</controls>
	</controller>
</MixxxMIDIPreset>

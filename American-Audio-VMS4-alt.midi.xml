<?xml version="1.0" encoding="utf-8"?>
<MixxxMIDIPreset schemaVersion="1" mixxxVersion="1.7.0+">
	<info>
		<name>American Audio VMS4</name>
		<author>xorik</author>
		<description>Alternative mapping for VMS4</description>
	</info>
	<controller id="American Audio VMS4">
		<controls>
			<!-- cue -->
			<control>
				<group>[Channel1]</group>
				<key>cue_default</key>
				<status>0x90</status>
				<midino>0x0c</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>cue_default</key>
				<status>0x80</status>
				<midino>0x0c</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>cue_default</key>
				<status>0x90</status>
				<midino>0x2e</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>cue_default</key>
				<status>0x80</status>
				<midino>0x2e</midino>
			</control>
			<!-- play/pause -->
			<control>
				<group>[Channel1]</group>
				<key>play</key>
				<status>0x90</status>
				<midino>0x0d</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>play</key>
				<status>0x90</status>
				<midino>0x2f</midino>
			</control>
			<!-- crossfader -->
			<control>
				<group>[Master]</group>
				<key>crossfader</key>
				<status>0xb0</status>
				<midino>0x1f</midino>
				<options>
					<invert/>
				</options>
			</control>
			<!-- volume/balance -->
			<control>
				<group>[Master]</group>
				<key>volume</key>
				<status>0xb0</status>
				<midino>0x1c</midino>
			</control>
			<control>
				<group>[Master]</group>
				<key>balance</key>
				<status>0xb0</status>
				<midino>0x1d</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>volume</key>
				<status>0xb0</status>
				<midino>0x09</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>volume</key>
				<status>0xb0</status>
				<midino>0x0e</midino>
			</control>
			<!-- rate -->
			<control>
				<group>[Channel1]</group>
				<key>rate</key>
				<status>0xe0</status>
				<midino>0x00</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>rate</key>
				<status>0xe1</status>
				<midino>0x00</midino>
			</control>
			<!-- eq -->
			<control>
				<group>[Channel1]</group>
				<key>filterHigh</key>
				<status>0xb0</status>
				<midino>0x06</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>filterMid</key>
				<status>0xb0</status>
				<midino>0x07</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>filterLow</key>
				<status>0xb0</status>
				<midino>0x08</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>filterHigh</key>
				<status>0xb0</status>
				<midino>0x0b</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>filterMid</key>
				<status>0xb0</status>
				<midino>0x0c</midino>
			</control>
			<control>
				<group>[Channel2]</group>
				<key>filterLow</key>
				<status>0xb0</status>
				<midino>0x0d</midino>
			</control>
		</controls>
	</controller>
</MixxxMIDIPreset>

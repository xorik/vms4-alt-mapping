			</control>
			<control>
				<group>[Channel1]</group>
				<key>hotcue_2_activate</key>
				<status>0x80</status>
				<midino>0x13</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>hotcue_3_activate</key>
				<status>0x90</status>
				<midino>0x14</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>hotcue_3_activate</key>
				<status>0x80</status>
				<midino>0x14</midino>
			</control>
			<control>
				<group>[Channel1]</group>
				<key>hotcue_4_activate</key>
